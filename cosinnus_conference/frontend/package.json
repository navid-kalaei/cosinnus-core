{
  "name": "wechange-conference",
  "productName": "wechange-conference",
  "version": "0.0.1",
  "description": "WECHANGE conference",
  "main": ".webpack/main",
  "scripts": {
    "format": "npm-run-all -c format:*",
    "format:eslint": "npm _eslint src/* --fix",
    "format:prettier": "npm _prettier --write",
    "lint": "npm-run-all lint:tsc lint:prettier lint:eslint",
    "lint:eslint": "npm _eslint",
    "lint:prettier": "npm _prettier --check",
    "lint:tsc": "tsc --noEmit",
    "_eslint": "eslint --no-error-on-unmatched-pattern \"./src/**/*.{js,jsx,ts,tsx,json,md,yaml,yml}\"",
    "_prettier": "prettier \"./src/*/*.{js,jsx,ts,tsx,json,md,yaml,yml}\"",
    "build": "npx webpack --config webpack.config.js",
    "watch": "npx webpack --watch --config webpack.config.js"
  },
  "keywords": [],
  "author": {
    "name": "Simon Liedtke",
    "email": "simon.liedtke@wechange.de"
  },
  "license": "AGPL 3.0",
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@material-ui/core": "^4.11.0",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "axios": "^0.20.0",
    "babel-core": "^6.26.3",
    "classnames": "^2.2.6",
    "clsx": "^1.1.1",
    "css-loader": "^4.3.0",
    "file-loader": "^6.1.0",
    "js-cookie": "^2.2.1",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "moment": "^2.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-iframe": "^1.8.0",
    "react-intl": "^5.8.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "style-loader": "^1.2.1",
    "styled-components": "^5.2.0",
    "svg-url-loader": "^6.0.0",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "dotenv-webpack": "^2.0.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-import-order-alphabetical": "^1.0.1",
    "eslint-plugin-jsdoc": "^30.4.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "fork-ts-checker-webpack-plugin": "^5.1.0",
    "npx": "^10.2.2",
    "react-hot-loader": "^4.12.21",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
